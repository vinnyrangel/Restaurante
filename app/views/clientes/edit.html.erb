<%= render :partial => "titulo" %>

<% form_for(@cliente) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :nome %><br />
    <%= f.text_field :nome %>
  </p>
  <p>
    <%= f.label :idade %><br />
    <%= f.text_field :idade %>
  </p>
  <p>
    <%= f.submit 'Atualizar' %>
  </p>
<% end %>

<%= link_to 'Exibir', @cliente %> |
<%= link_to 'Voltar', clientes_path %>
